    <string name="auto_add_shortcuts_description">Agregar automáticamente atajos a las apps recién instaladas al escritorio</string>

    <!-- Developer setting to change the shape of icons on home screen. [CHAR LIMIT=50] -->
    <string name="icon_shape_override_label">Cambiar forma de iconos</string>
